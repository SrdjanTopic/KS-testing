<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns="http://www.example.org/ontology/sotis#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Ontology rdf:about="http://www.example.org/ontology/sotis"/>
  <owl:Class rdf:about="http://www.example.org/ontology/sotis#Student">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.example.org/ontology/sotis#User"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.example.org/ontology/sotis#Teacher">
    <rdfs:subClassOf rdf:resource="http://www.example.org/ontology/sotis#User"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.example.org/ontology/sotis#Answer"/>
  <owl:Class rdf:about="http://www.example.org/ontology/sotis#Test"/>
  <owl:Class rdf:about="http://www.example.org/ontology/sotis#Concept"/>
  <owl:Class rdf:about="http://www.example.org/ontology/sotis#Question"/>
  <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#questionConcept">
    <rdfs:range>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Concept"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#questionConcept"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Question"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#questionConcept"/>
      </owl:Restriction>
    </rdfs:domain>
    <rdfs:comment>A Question belongs to a Concept.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#answerStudent">
    <rdfs:range>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Student"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#answerStudent"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Answer"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#answerStudent"/>
      </owl:Restriction>
    </rdfs:domain>
    <rdfs:comment>An Answer can be answered by multiple Students</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#questionTest">
    <rdfs:range>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Test"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#questionTest"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Question"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#questionTest"/>
      </owl:Restriction>
    </rdfs:domain>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#testQuestions"/>
    </owl:inverseOf>
    <rdfs:comment>Every Question belongs to a Test.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#studentAnswer">
    <rdfs:domain>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Student"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#studentAnswer"/>
      </owl:Restriction>
    </rdfs:domain>
    <rdfs:range>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Answer"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#studentAnswer"/>
      </owl:Restriction>
    </rdfs:range>
    <owl:inverseOf rdf:resource="http://www.example.org/ontology/sotis#answerStudent"/>
    <rdfs:comment>A Student can have 0 or more Answers.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#testTeacher">
    <rdfs:domain>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Test"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#testTeacher"/>
      </owl:Restriction>
    </rdfs:domain>
    <rdfs:range>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Teacher"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#testTeacher"/>
      </owl:Restriction>
    </rdfs:range>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#teacherTest"/>
    </owl:inverseOf>
    <rdfs:comment>Every test is created by a Teacher</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#answerQuestion">
    <rdfs:range>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Question"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#answerQuestion"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Answer"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#answerQuestion"/>
      </owl:Restriction>
    </rdfs:domain>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#questionAnswers"/>
    </owl:inverseOf>
    <rdfs:comment>Every Answer belongs to a Question</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#teacherTest">
    <rdfs:range>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Test"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#teacherTest"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Teacher"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#teacherTest"/>
      </owl:Restriction>
    </rdfs:domain>
    <rdfs:comment>A Teacher can create multiple Tests.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#conceptQuestion">
    <rdfs:domain>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Concept"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#conceptQuestion"/>
      </owl:Restriction>
    </rdfs:domain>
    <rdfs:range>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Question"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#conceptQuestion"/>
      </owl:Restriction>
    </rdfs:range>
    <owl:inverseOf rdf:resource="http://www.example.org/ontology/sotis#questionConcept"/>
    <rdfs:comment>Every Concept can have multiple corresponding Questions.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#questionAnswers">
    <rdfs:range>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >2</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Answer"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#questionAnswers"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:domain>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Question"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#questionAnswers"/>
      </owl:Restriction>
    </rdfs:domain>
    <rdfs:comment>Every Question must have at least 2 Answer</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.example.org/ontology/sotis#testQuestions">
    <rdfs:domain>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Test"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#testQuestions"/>
      </owl:Restriction>
    </rdfs:domain>
    <rdfs:range>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Question"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#testQuestions"/>
      </owl:Restriction>
    </rdfs:range>
    <rdfs:comment>Every test must have at least 1 Question</rdfs:comment>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="http://www.example.org/ontology/sotis#userFirstName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://www.example.org/ontology/sotis#User"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.example.org/ontology/sotis#questionQuestion">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://www.example.org/ontology/sotis#Question"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.example.org/ontology/sotis#userLastName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://www.example.org/ontology/sotis#User"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.example.org/ontology/sotis#answerAnswer">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://www.example.org/ontology/sotis#Answer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.example.org/ontology/sotis#conceptName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://www.example.org/ontology/sotis#Concept"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.example.org/ontology/sotis#answerIsCorrect">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="http://www.example.org/ontology/sotis#Answer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.example.org/ontology/sotis#testName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://www.example.org/ontology/sotis#Test"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.example.org/ontology/sotis#id">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.example.org/ontology/sotis#username">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="http://www.example.org/ontology/sotis#User"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://www.example.org/ontology/sotis#questionPoints">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="http://www.example.org/ontology/sotis#Question"/>
  </owl:DatatypeProperty>
  <owl:TransitiveProperty rdf:about="http://www.example.org/ontology/sotis#isSourceFor">
    <rdfs:domain>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Concept"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#isSourceFor"/>
      </owl:Restriction>
    </rdfs:domain>
    <rdfs:range>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Concept"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#isSourceFor"/>
      </owl:Restriction>
    </rdfs:range>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:TransitiveProperty>
  <owl:TransitiveProperty rdf:about="http://www.example.org/ontology/sotis#isDestinationFor">
    <rdfs:domain>
      <owl:Restriction>
        <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:qualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Concept"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#isDestinationFor"/>
      </owl:Restriction>
    </rdfs:domain>
    <rdfs:range>
      <owl:Restriction>
        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >0</owl:minQualifiedCardinality>
        <owl:onClass rdf:resource="http://www.example.org/ontology/sotis#Concept"/>
        <owl:onProperty rdf:resource="http://www.example.org/ontology/sotis#isDestinationFor"/>
      </owl:Restriction>
    </rdfs:range>
    <owl:inverseOf rdf:resource="http://www.example.org/ontology/sotis#isSourceFor"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:TransitiveProperty>

</rdf:RDF>
